    "express-validator": "^6.9.2",
    "mongoose": "^5.11.15"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7"
  },
  "nodemonConfig": {
    "verbose": false,
    "ignore": [
      "README.md",
      "client"
    ],
    "ext": "js, css",
    "restartable": "rs"
