{
  "name": "holidog",
  "version": "1.0.0",
  "description": "assessment for Holidog",
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "server": "nodemon server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server \" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Andrei Ce",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "config": "^3.3.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^6.9.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.15",
    "normalize-url": "^5.3.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "chai": "^4.3.0",
    "mocha": "^8.3.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3"
  },
  "nodemonConfig": {
    "verbose": false,
    "ignore": [
      "README.md",
      "client"
    ],
    "ext": "js, css",
    "restartable": "rs"
  }
}
